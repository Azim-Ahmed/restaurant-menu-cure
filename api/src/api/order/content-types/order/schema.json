{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "orderId": {
      "type": "text",
      "required": true,
      "minLength": 10,
      "unique": true
    },
    "dateTime": {
      "type": "datetime"
    },
    "order_status": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order-status.order-status"
    },
    "status": {
      "type": "integer",
      "default": 1
    },
    "totalPrice": {
      "type": "biginteger"
    },
    "foods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::food.food",
      "mappedBy": "order"
    },
    "payment": {
      "type": "boolean",
      "default": false
    },
    "table": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::table.table",
      "mappedBy": "order"
    },
    "discount": {
      "type": "integer"
    },
    "final_price": {
      "type": "biginteger"
    }
  }
}
