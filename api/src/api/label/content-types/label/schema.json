{
  "kind": "collectionType",
  "collectionName": "labels",
  "info": {
    "singularName": "label",
    "pluralName": "labels",
    "displayName": "Label"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3
    },
    "sign": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": false,
      "private": true
    },
    "foods": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::food.food",
      "mappedBy": "labels"
    }
  }
}
